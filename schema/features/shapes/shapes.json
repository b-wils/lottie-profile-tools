{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "allOf": [{
        "$ref": "#/$defs/all-graphic-elements"
    }],
    "$defs": {
        "all-graphic-elements": {
            "oneOf": [
                {
                    "$ref": "#/$defs/group"
                },
                {
                    "$ref": "#/$defs/other-shapes"
                }
            ],
            "allOf": [
                {
                    "$ref": "rectangle-check.json"
                }
            ]
        },
        "group": {
            "type": "object",
            "properties": {
                "ty": {
                    "const": "gr"
                },
                "it": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/all-graphic-elements"
                    }
                }
            }
        },
        "other-shapes": {
            "type": "object",
            "properties": {
                "ty": {
                    "type": "string",
                    "not": {
                        "oneOf": [
                            {
                                "const": "gr"
                            }
                        ]
                    }
                }
            }
        }
    }
}